{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "jest",
    "temp": "open 'http://www.google.ca' && which java"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "expo": "^46.0.14",
    "fela": "^10.1.0",
    "fela-native": "^10.1.1",
    "fela-tools": "^10.1.0",
    "prop-types": "^15.6.2",
    "randomcolor": "^0.5.3",
    "react": "16.5.0",
    "react-fela": "10.1.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-navigation": "2.5.5",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/react-test-renderer": "^16.0.3",
    "babel-7-jest": "^21.3.3",
    "babel-preset-expo": "^5.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest": "^23.6.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^3.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.m?jsx?$": "babel-jest"
    },
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/__tests__/setup"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/__tests__/setup.js"
    ]
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "private": true
}
